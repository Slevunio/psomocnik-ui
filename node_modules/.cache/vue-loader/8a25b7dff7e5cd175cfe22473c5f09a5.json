50px;\">\n        <div class=\"col-12 text-center\">\n            <h1 class=\"display-3\">Edytuj prawa dostępu</h1>\n        </div>\n        <div class=\"container users\">\n            <table class=\"table\">\n                <thead>\n                <tr>\n                    <th>Id</th>\n                    <th>Nazwa użytkownika</th>\n                    <th>Email</th>\n                    <th>Prawa</th>\n                    <th>Data stworzenia</th>\n                    <th>Data ostatniej edycji</th>\n                </tr>\n                </thead>\n                <tbody>\n                <tr :id=\"user.id\" v-for=\"user in users\">\n                    <td>{{user.id}}</td>\n                    <td>{{user.username}}</td>\n                    <td>{{user.email}}</td>\n                    <td>{{user.role}}</td>\n                    <td>{{user.created}}</td>\n                    <td>{{user.lastChanged}}</td>\n                    <td><router-link :to=\"{name: 'editUser', params: {userId: user.id}}\" tag=\"button\" class=\"btn btn-psomocnik\" :id=\"'edit'+user.id\">Edytuj</router-link></td>\n                    <td>\n                        <button type=\"button\" class=\"btn btn-psomocnik\" :id=\"'delete'+user.id\"\n                                v-on:click.capture=\"setId(user.id)\" data-toggle=\"modal\" data-target=\"#deleteModal\">\n                            Usuń\n                        </button>\n                    </td>\n                </tr>\n                </tbody>\n            </table>\n            <a class=\"btn btn-psomocnik btn-lg\" href=\"addUser\" id=\"add\">Dodaj</a>\n\n            <div class=\"modal fade\" id=\"deleteModal\">\n                <div class=\"modal-dialog\">\n                    <div class=\"modal-content\">\n                        <div class=\"modal-body text-center\">\n                            <h4>Usunąć?</h4>\n                        </div>\n                        <div class=\"modal-footer\">\n                            <div class=\"col-6 text-center\">\n                                <button type=\"submit\" class=\"btn btn-psomocnik\" data-dismiss=\"modal\"\n                                        v-on:click.capture=\"deleteUser()\">Ok\n                                </button>\n                            </div>\n                            <div class=\"col-6 text-center\">\n                                <button type=\"button\" class=\"btn btn-psomocnik\" data-dismiss=\"modal\">Anuluj</button>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import api from \"../backend-api\";\n\n    export default {\n        name: \"ManageUsers\",\n        data(){\n            return{\n                users:[],\n                id:''\n            }\n        },\n        mounted(){\n            this.readUsers();\n        },\n        methods:{\n            readUsers(){\n                api.readUsers().then(response=>{\n                    this.users=response.data;\n                });\n            },\n            setId(id){\n                this.id=id;\n            },\n            deleteUser(){\n                api.deleteUser(this.id).then(\n                    document.location.replace(\"/manageUsers\")\n                );\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}